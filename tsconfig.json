{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "es6", "esnext.asynciterable"],
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "plugins": [{ "name": "typescript-tslint-plugin" }],
    "types": ["jest"]
  }
}
